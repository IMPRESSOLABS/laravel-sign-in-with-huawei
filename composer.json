{
    "name": "impressolabs/laravel-sign-in-with-huawei",
    "description": "Add Huawei's new single-signon feature to your site with ease.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stark Liew",
            "email": "hello@impressolabs.io",
            "homepage": "https://www.impressoapp.com/",
            "role": "Developer"
        }
    ],
    "readme": "README.md",
    "require": {
        "illuminate/support": "^7.0",
        "laravel/socialite": "^4.3"
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^5.0",
        "orchestra/testbench-dusk": "^5.0",
        "orchestra/testbench": "^5.0",
        "phpmd/phpmd": "^2.7",
        "squizlabs/php_codesniffer": "^3.5",
        "predis/predis": "^1.1",
        "phpunit/phpunit": "9.3"
    },
    "autoload": {
        "psr-4": {
            "ImpressoLabs\\LaravelSignInWithHuawei\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ImpressoLabs\\LaravelSignInWithHuawei\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\ImpressoLabs\\LaravelSignInWithHuawei\\Providers\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

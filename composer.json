{
    "name": "impressolabs/laravel-sign-in-with-huawei",
    "description": "Add Huawei's new single-signon feature to your site with ease.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Stark Liew",
            "email": "hello@impressolabs.io",
            "homepage": "https://www.impressoapp.com/",
            "role": "Developer"
        }
    ],
    "readme": "README.md",
    "require": {
        "php": ">=7.3",
        "illuminate/support": "^8.0",
        "laravel/socialite": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "ImpressoLabs\\LaravelSignInWithHuawei\\": "src/"
        }
    },
    "require-dev": {
        "orchestra/testbench-browser-kit": "^6.0",
        "orchestra/testbench-dusk": "^6.0",
        "orchestra/testbench": "^6.0",
        "predis/predis": "^1.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload-dev": {
        "psr-4": {
            "ImpressoLabs\\LaravelSignInWithHuawei\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "\\ImpressoLabs\\LaravelSignInWithHuawei\\Providers\\ServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
